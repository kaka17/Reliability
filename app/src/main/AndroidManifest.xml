<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kaiser.reliability">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name="com.kaiser.reliability.base.AppContext"
        android:allowBackup="true"
        android:icon="@mipmap/icon01"
        android:label="@string/app_name"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">

        <!-- 屏幕适配相关  这里标UI出图的尺寸 -->
        <meta-data
            android:name="design_width"
            android:value="768" />
        <meta-data
            android:name="design_height"
            android:value="1280" />

        <activity
            android:name="com.kaiser.reliability.MainActivity"
            android:label="@string/app_name" />
        <activity
            android:name="com.kaiser.reliability.activity.KaiserGuInfoActivity"
            android:label="@string/title_activity_Kaiser" />
        <activity android:name="com.kaiser.reliability.activity.KKLineActivity" />
        <activity android:name="com.kaiser.reliability.activity.LoginActivity"></activity>
        <activity
            android:name="com.kaiser.reliability.activity.ToLoanActivity"
            android:label="@string/app_name"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.kaiser.reliability.activity.RegistActivity"
            >

        </activity>
        <activity android:name="com.kaiser.reliability.activity.MyInfoActivity"></activity>
    </application>

</manifest>